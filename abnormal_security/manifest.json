{
    "name": "Abnormal Security",
    "id": "abnormal_security",
    "version": "1.1.0",
    "description": "This plugin is used to pull IOCs of type SHA256, MD5, URL, Domain, IPv4 and IPv6 from Threat Log page of Abnormal Security platform. This plugin supports retraction of IOCs pulled from Abnormal Security platform. This plugin does not support sharing of indicators to Abnormal Security platform.",
    "minimum_version": "5.1.0",
    "patch_supported": false,
    "push_supported": false,
    "fetch_retraction_info": true,
    "module": "CTE",
    "configuration": [
        {
            "label": "Base URL",
            "key": "base_url",
            "type": "choice",
            "choices": [
                {
                    "key": "Production Server (api.abnormalplatform.com)",
                    "value": "https://api.abnormalplatform.com/v1"
                },
                {
                    "key": "EU Production Server (eu.rest.abnormalsecurity.com)",
                    "value": "https://eu.rest.abnormalsecurity.com/v1"
                },
                {
                    "key": "FedRAMP GovCloud (rest.abnormalsecurity.us )",
                    "value": "https://rest.abnormalsecurity.us"
                }
            ],
            "mandatory": true,
            "default": "https://api.abnormalplatform.com/v1",
            "description": "Base URL of Abnormal Security API endpoint."
        },
        {
            "label": "API Token",
            "key": "api_key",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "Provide the API Token from Settings > Integrations > Abnormal REST API"
        },
        {
            "label": "Type of Threat data to pull",
            "key": "type",
            "type": "multichoice",
            "choices": [
                {
                    "key": "SHA256",
                    "value": "sha256"
                },
                {
                    "key": "MD5",
                    "value": "md5"
                },
                {
                    "key": "URL",
                    "value": "url"
                },
                {
                    "key": "Domain",
                    "value": "domain"
                },
                {
                    "key": "IPv4",
                    "value": "ipv4"
                },
                {
                    "key": "IPv6",
                    "value": "ipv6"
                }
            ],
            "default": [
                "sha256",
                "md5",
                "url",
                "domain",
                "ipv4",
                "ipv6"
            ],
            "mandatory": true,
            "description": "Type of Threat data to pull. Allowed values are SHA256, MD5, URL, Domain, IPv4 and IPv6."
        },
        {
            "label": "Retraction Interval (in days)",
            "key": "retraction_interval",
            "type": "number",
            "mandatory": false,
            "description": "Specify the number of days for which IoC retraction should be run for Abnormal Security indicators. Note: This parameter is applicable only for Netskope CE version 5.1.0 or later, and if \"IoC(s) Retraction\" is enabled in Threat Exchange Settings. Value must be between 1 and 2^62."
        },
        {
            "label": "Enable Tagging",
            "key": "enable_tagging",
            "type": "choice",
            "choices": [
                {
                    "key": "Yes",
                    "value": "yes"
                },
                {
                    "key": "No",
                    "value": "no"
                }
            ],
            "default": "yes",
            "mandatory": true,
            "description": "Enable/Disable tagging functionality."
        },
        {
            "label": "Initial Range (in days)",
            "key": "initial_pull_range",
            "type": "number",
            "mandatory": true,
            "default": 7,
            "description": "Number of days to pull the data for the initial run. Value must be between 0 and 2^62."
        }
    ]
}