{
    "name": "Amazon Security Lake",
    "id": "amazon_security_lake",
    "version": "1.0.0",
    "mapping": "Amazon Security Lake Default Mappings",
    "netskope": false,
    "types": [
        "alerts",
        "events",
        "webtx"
    ],
    "description": "This plugin is used to deliver alerts, events and web transactions data to Amazon Security Lake platform. To access the plugin, you would need the credentials of Amazon Security Lake instance.",
    "configuration": [
        {
            "label": "AWS Access Key ID (Public Key)",
            "key": "aws_public_key",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "AWS access key id for Amazon Security Lake authentication."
        },
        {
            "label": "AWS Secret Access Key (Private Key)",
            "key": "aws_private_key",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "AWS secret access key for Amazon Security Lake authentication."
        },
        {
            "label": "Region Name",
            "key": "region_name",
            "type": "choice",
            "choices": [
                {
                    "key": "US East (N. Virginia) [us-east-1]",
