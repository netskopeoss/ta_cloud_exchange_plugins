{
    "name": "Cybereason",
    "id": "cybereason_plugin",
    "version": "1.0.1",
    "description": "This plugin is used to fetch File hashes and URL from Cybereason's Custom IOC. This plugin can share the indicator Hash or URL to Cybereason's Custom IOC. To access the plugin, you would need the API credentials. Refer the Cybereason documentation https://nest.cybereason.com/documentation/api-documentation/all-versions/set-reputation#setreputations for more details.",
    "push_supported": true,
    "patch_supported": true,
    "configuration": [
        {
            "label": "Base URL",
            "key": "base_url",
            "type": "text",
            "default": "https://integration.cybereason.net:8443",
            "mandatory": true,
            "description": "The URL of the Cybereason console from which we want to fetch the data."
        },
        {
            "label": "Username",
            "key": "username",
            "type": "text",
            "default": "",
            "mandatory": true,
            "description": "API Username."
        },
        {
            "label": "Password",
            "key": "password",
            "type": "password",
            "default": "",
            "mandatory": true,
            "description": "API Password."
        },
        {
            "label": "Enable Polling",
            "key": "is_pull_required",
            "type": "choice",
            "choices": [
                {
                    "key": "Yes",
                    "value": "Yes"
                },
                {
                    "key": "No",
                    "value": "No"
                }
            ],
            "default": "Yes",
            "mandatory": true,
            "description": "Enable/Disable polling data from Cybereason."
        }
    ]
}
    